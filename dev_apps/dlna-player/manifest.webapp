{
  "name": "dlnaPlayer",
  "description": "Gaia dlna player",
  "type": "certified",
  "launch_path": "/easyPlayer/index.html",
  "developer": {
    "name": "Evan Shapiro",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": {
    "storage": {},
    "device-storage:music": {
      "access": "readwrite"
    },
    "settings": {
      "access": "readonly"
    },
    "deprecated-hwvideo": {},
    "audio-channel-content": {},
    "systemXHR": {},
    "bluetooth": {}
  },
  "locales": {
    "ar": {
      "name": "DLNA",
      "description": "Gaia DLNA"
    },
    "en-US": {
      "name": "DLNA",
      "description": "Gaia DLNA"
    },
    "fr": {
      "name": "DLNA",
      "description": "Gaia DLNA"
    },
    "zh-TW": {
      "name": "DLNA",
      "description": "Gaia DLNA"
    }
  },
  "default_locale": "en-US",
  "icons": {
    "60": "/style/icons/Music_60.png",
    "90": "/style/icons/Music_90.png",
    "120": "/style/icons/Music_120.png"
  },
  "orientation": "default"
}
